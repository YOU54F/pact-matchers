{
  "consumer": {
    "name": "Example Bi-Directional Consumer"
  },
  "interactions": [
    {
      "description": "a request to retrieve a product id that does not exist",
      "request": {
        "method": "GET",
        "path": "/Products/10"
      },
      "response": {
        "headers": {
          "Content-Type": "application/json; charset=utf-8"
        },
        "status": 404
      }
    },
    {
      "description": "a request to retrieve a product with existing ids",
      "request": {
        "method": "GET",
        "path": "/Products/27"
      },
      "response": {
        "body": {
          "id": "27",
          "name": "burger",
          "type": "food"
        },
        "headers": {
          "Content-Type": "application/json; charset=utf-8"
        },
        "matchingRules": {
          "body": {
            "$.id": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "type"
                }
              ]
            },
            "$.name": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "type"
                }
              ]
            },
            "$.type": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "type"
                }
              ]
            }
          },
          "header": {}
        },
        "status": 200
      }
    },
    {
      "description": "a request to retrieve all productss",
      "request": {
        "method": "GET",
        "path": "/Products"
      },
      "response": {
        "body": [
          {
            "id": "27",
            "name": "burger",
            "type": "food"
          }
        ],
        "headers": {
          "Content-Type": "application/json; charset=utf-8"
        },
        "matchingRules": {
          "body": {
            "$": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "type",
                  "min": 1
                }
              ]
            }
          },
          "header": {}
        },
        "status": 200
      }
    }
  ],
  "metadata": {
    "pactRust": {
      "ffi": "0.2.1",
      "mockserver": "0.8.7",
      "models": "0.2.7"
    },
    "pactSpecification": {
      "version": "3.0.0"
    }
  },
  "provider": {
    "name": "ExampleProvider"
  }
}
