{
  "provider": {
    "name": "Alice Service"
  },
  "consumer": {
    "name": "Consumer"
  },
  "interactions": [
    {
      "description": "a retrieve Mallory request",
      "request": {
        "method": "GET",
        "path": "/mallory",
        "query": {
          "name": ["ron"],
          "status": ["good"]
        }
      },
      "response": {
        "status": 200,
        "headers": {
          "Content-Type": ["text/html"]
        },
        "body": "\"That is some good Mallory.\""
      }
    }
  ],
  "metadata": {
    "pactSpecification": {
      "version": "3.0.0"
    },
    "pact-jvm": {
      "version": "3.0.0"
    }
  }
}
