{
  "consumer": {
    "name": "SNSPactEventConsumer"
  },
  "messages": [
    {
      "contents": {
        "event": "something important",
        "id": 1,
        "type": "save"
      },
      "description": "a request to save an event",
      "matchingRules": {
        "body": {
          "$.event": {
            "combine": "AND",
            "matchers": [
              {
                "match": "type"
              }
            ]
          },
          "$.id": {
            "combine": "AND",
            "matchers": [
              {
                "match": "type"
              }
            ]
          },
          "$.type": {
            "combine": "AND",
            "matchers": [
              {
                "match": "regex",
                "regex": "^(save|update|cancel)$"
              }
            ]
          }
        }
      },
      "metadata": {
        "content-type": "application/json",
        "contentType": "application/json"
      }
    }
  ],
  "metadata": {
    "pact-js": {
      "version": "10.1.2"
    },
    "pactRust": {
      "ffi": "0.3.8",
      "models": "0.4.4"
    },
    "pactSpecification": {
      "version": "3.0.0"
    }
  },
  "provider": {
    "name": "SNSPactEventProvider"
  }
}